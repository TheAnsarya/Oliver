# To learn more about .editorconfig see https://aka.ms/editorconfigdocs
###############################
# Core EditorConfig Options   #
###############################
root = true
# All files
[*]
indent_style = tab
end_of_line = crlf
insert_final_newline = true
indent_size = 4
###############################
# Json Formatting Rules       #
###############################
[*.json]
indent_style = tab
indent_size = 4

